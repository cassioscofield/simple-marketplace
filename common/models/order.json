{
  "name": "Order",
  "base": "PersistedModel",
  "idInjection": true,
  "forceId": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "orderId": {
      "type": "string",
      "id": true,
      "index": true,
      "isArray": false,
      "generated": true
    },
    "orderDate": {
      "type": "date",
      "default": "$now"
    },
    "ammuntPaid": {
      "type": "number",
      "dataType": "decimal",
      "precision": 10,
      "scale": 2,
      "required": false
    },
    "marketplaceFee": {
      "type": "number",
      "dataType": "decimal",
      "precision": 10,
      "scale": 2,
      "required": false
    },
    "paymentFee": {
      "type": "number",
      "dataType": "decimal",
      "precision": 10,
      "scale": 2,
      "required": false
    },
    "storeRevenue": {
      "type": "number",
      "dataType": "decimal",
      "precision": 10,
      "scale": 2,
      "required": false
    },
    "status": {
      "type": "string",
      "default": "active"
    }
  },
  "relations": {
    "product": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": "productId"
    },
    "store": {
      "type": "belongsTo",
      "model": "Store",
      "foreignKey": "storeId"
    }
  },
  "foreignKeys": {
    "productId": {
      "name": "productIdInOrder",
      "foreignKey": "productId",
      "entityKey": "productId",
      "entity": "Product"
    },
    "storeId": {
      "name": "storeIdInOrder",
      "foreignKey": "storeId",
      "entityKey": "storeId",
      "entity": "Store"
    }
  }
}
