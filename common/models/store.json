{
  "name": "Store",
  "base": "PersistedModel",
  "idInjection": true,
  "forceId": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "storeId": {
      "type": "string",
      "id": true,
      "index": true,
      "isArray": false,
      "generated": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "marketplaceFee": {
      "type": "number",
      "default": 0.09
    },
    "paymentFee": {
      "type": "number",
      "default": 0.01
    },
    "status": {
      "type": "string",
      "default": "active"
    }
  },
  "relations": {
    "products": {
      "type": "hasMany",
      "model": "Product",
      "foreignKey": ""
    },
    "orders": {
      "type": "hasMany",
      "model": "Order",
      "foreignKey": ""
    }
  }
}
